# vim:ft=yaml

extends: airbnb
env:
  node: true,
rules:
  no-eq-null: 2
  no-unused-vars: 2
  no-bitwise: 2
  eqeqeq: 2
  guard-for-in: 2
  no-extend-native: 2
  no-caller: 2
  max-depth:
    - 2
    - 2
  linebreak-style: 2
  no-undef: 2
  no-unused-expressions: 2
  comma-dangle:
    - 2
    - never
  import/no-unresolved: 0
